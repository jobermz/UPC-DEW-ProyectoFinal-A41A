<h1>Listado de Distritos</h1>

<table>
  <tr>
    <th>Nombre del Distrito</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @districts.each do |district| %>
  <tr>
    <td><%= district.district_name %></td>
    <td><%= link_to 'Mostrar', district %></td>
    <td><%= link_to 'Editar', edit_district_path(district) %></td>
    <td><%= link_to 'Eliminar', district, method: :delete, data: { confirm: 'Esta seguro de Eliminar?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Agregar Distrito', new_district_path %>
